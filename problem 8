#include <iostream>
#include <conio.h>

using namespace std;

int main()

{
	int a, b = 1; 
	
	cout << "Enter a number: ";	cin >> a;
    cout << "The multiplication table of " << a << ":" << endl;
    
    for (b; b <= 10; b++) 
		{
        	cout << a << " * " << b << " = " << b * a << endl; 
    	}
    	

	return 0;
}
